<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_job_recruitment_survey_inherit" model="ir.ui.view">
        <field name="name">hr.job.recruitment.form.inherit</field>
        <field name="model">hr.job</field>
        <field name="inherit_id" ref="hr_recruitment.hr_job_survey"/>
        <field name="arch" type="xml">
            <field name="interviewer_ids" position="after">
                <field name="recruitment_survey_id" context="{'default_access_mode': 'token'}"/>
            </field>
        </field>
    </record>
</odoo>
